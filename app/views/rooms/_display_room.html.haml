.formatted_room
  %li
    %h6
      Room ##{room.room_num}
    =render 'shared/display_room_and_view_types',room_types:room_types,view_types:view_types
    %img(src = 'data:image/jpeg;base64,')
    %h6
      =link_to 'Make a reservation!', '/room_reservation/new/'+room.id.to_s
    %h6 #{format_room_price(room.price)}$/night
